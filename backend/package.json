{
  "name": "@autoline/backend",
  "private": true,
  "engines": {
    "node": ">=16 <=18",
    "yarn": ">=1 <2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "lint:js": "npx eslint \"src/**/*.ts\" --resolve-plugins-relative-to .",
    "lint": "yarn lint:js",
    "start:dev": "nodemon --exec ts-node --files -r tsconfig-paths/register ./src/server.ts",
    "start:dev:local": "dotenv -e ../.env/backend.env -- yarn start:dev",
    "start:prod": "node ./build/src/server.js",
    "build": "tsc && tsc-alias -p tsconfig.json",
    "db:generate": "yarn prisma generate",
    "db:generate:local": "dotenv -e ../.env/backend.env -- yarn db:generate",
    "db:push": "dotenv -e ../.env/backend.env -- yarn prisma db push",
    "db:create-migration": "dotenv -e ../.env/backend.env -- yarn prisma migrate dev --name prices_range_table",
    "db:reset": "dotenv -e ../.env/backend.env -- yarn prisma migrate reset",
    "db:migrate:dev": "yarn prisma migrate dev",
    "db:migrate:dev:local": "dotenv -e ../.env/backend.env -- yarn db:migrate:dev",
    "db:migrate:prod": "yarn prisma migrate deploy"
  },
  "dependencies": {
    "@autoline/shared": "link:../shared/build",
    "@prisma/client": "4.1.1",
    "@sentry/node": "^7.9.0",
    "@sentry/tracing": "^7.9.0",
    "@types/nodemailer": "^6.4.5",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.1",
    "@types/passport": "^1.0.9",
    "@types/passport-local": "^1.0.34",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.1",
    "ts-node": "^10.8.2",
    "tsc-alias": "^1.6.11",
    "typescript": "4.7.4"
  }
}
