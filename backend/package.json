{
  "name": "@autoline/backend",
  "private": true,
  "engines": {
    "node": ">=16 <=18",
    "yarn": ">=1 <2"
  },
  "scripts": {
    "lint:js": "npx eslint \"src/**/*.ts\" --resolve-plugins-relative-to .",
    "lint": "yarn lint:js",
    "start:dev": "nodemon --exec ts-node --files -r tsconfig-paths/register ./src/server.ts",
    "start:prod": "node ./build/src/server.js",
    "build": "tsc && tsc-alias -p tsconfig.json"
  },
  "dependencies": {
    "autoline-shared": "file:../shared/build",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.1",
    "@types/passport": "^1.0.9",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.2",
    "tsc-alias": "^1.6.11",
    "typescript": "4.7.4"
  }
}
